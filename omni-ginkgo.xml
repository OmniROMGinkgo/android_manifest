<?xml version="1.0" encoding="UTF-8"?>
<manifest>

  <!-- OmniROM Ginkgo remote -->
  <remote  name="omni-ginkgo"
           fetch="https://github.com/OmniROMGinkgo/"
           revision="android-10" />

  <!-- OmniROM Ginkgo REPOS -->

  <!-- General -->
  <project path="build/make" name="android_build" remote="omni-ginkgo" >
    <copyfile src="core/root.mk" dest="Makefile" />
    <linkfile src="CleanSpec.mk" dest="build/CleanSpec.mk" />
    <linkfile src="buildspec.mk.default" dest="build/buildspec.mk.default" />
    <linkfile src="core" dest="build/core" />
    <linkfile src="envsetup.sh" dest="build/envsetup.sh" />
    <linkfile src="target" dest="build/target" />
    <linkfile src="tools" dest="build/tools" />
  </project>
  <project path="device/omni/sepolicy" name="android_device_omni_sepolicy" remote="omni-ginkgo" />
  <project path="device/qcom/sepolicy" name="android_device_qcom_sepolicy" remote="omni-ginkgo" />
  <project path="device/qcom/sepolicy-legacy" name="android_device_qcom_sepolicy" remote="omni-ginkgo" revision="android-10-legacy" />
  <project path="device/qcom/sepolicy-legacy-um" name="android_device_qcom_sepolicy" remote="omni-ginkgo" revision="android-10-legacy-um" />
  <project path="manifest" name="android_manifest" remote="omni-ginkgo" />

  <!-- External -->
  <project path="external/ant-wireless/ant_native" name="android_external_ant-wireless_ant_native" remote="omni-ginkgo" />
